{
  "name": "lorecal",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "install:react": "cd client && npm install --production",
    "gcp-build": "npm run-script install:react",
    "build": "npm run build --prefix ./client & npm run-script migrate",
    "start": "npm run-script start:express & npm run-script start:react",
    "start:react": "npm start --prefix ./client",
    "start:express": "node ./bin/www",
    "migrate": "node db/migrations.js up",
    "migrate:down": "node db/migrations.js down"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0",
    "mysql": "^2.16.0",
    "mysql-migrations": "^1.0.6",
    "node-sass-middleware": "0.11.0"
  }
}
